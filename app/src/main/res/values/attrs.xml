<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GPUImageView">
        <attr name="gpuimage_surface_type" format="enum">
            <enum name="surface_view" value="0" />
            <enum name="texture_view" value="1" />
        </attr>
        <attr name="gpuimage_show_loading" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="IconCropView">
        <attr name="minimumSide" format="dimension"/>
        <attr name="resizeCornerDrawable" format="reference"/>
        <attr name="moveCornerDrawable" format="reference"/>
        <attr name="cornerColor" format="color"/>
        <attr name="edgeColor" format="color" />
        <attr name="outsideCropColor" format="color" />
        <attr name="cropOverlayCornerSize" format="dimension" />
    </declare-styleable>

</resources>